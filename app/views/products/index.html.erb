<h1>Products</h1>
<%= will_paginate @products %>
<div class="table-responsive">
 <table class="table table-striped table-hover">
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>URL</th>
      <th>Price</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @products.each do |product| %>
     <tr>
       <td><%= "photo" %></td>
       <td><%= link_to product.title, user_feed_product_path(current_user, @feed, product) %></td>
       <td><%= link_to product.url, product.url %></td>
       <td><%= "$#{product.price}" %></td>
       <td><%= link_to raw("<span class='glyphicon glyphicon-pencil' aria-hidden='true'></span><b class='button-text'>Update Product Information</b>"), edit_user_feed_product_path(current_user, @feed, product, page: params[:page]), class: "btn btn-primary btn-sm icon-button" %></td>
       <td><%= link_to raw("<span class='glyphicon glyphicon-remove' aria-hidden='true'></span><b class='button-text'>Delete Product</b>"), user_feed_product_path(current_user, @feed, product, page: params[:page]), method: :delete, class: "btn btn-danger btn-sm icon-button" %></td>
     </tr>
  <% end %>
  </tbody>
 </table>
</div>
<%= will_paginate @products %>
